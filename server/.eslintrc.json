{
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "no-unused-vars": [
      "error",
      {
        // Ignore arguments starting with _
        "argsIgnorePattern": "^_"
      }
    ]
  },
  "extends": "eslint:recommended"
}
